{
  "name": "redux-todo",
  "version": "1.0.0",
  "description": "Simple redux todo app",
  "main": "index.html",
  "scripts": {
    "build": "echo '=> building' && NODE_ENV=production ./node_modules/.bin/webpack --progress",
    "lint": "echo '=> linting' && ./node_modules/.bin/eslint src",
    "prebuild": "npm run lint -s",
    "start": "open index.html",
    "test": "mocha --compilers js:babel-core/register --recursive \"./src/*.spec.js\"",
    "test:watch": "npm test -- --watch",
    "watch": "NODE_ENV=development ./node_modules/.bin/webpack --watch --progress"
  },
  "author": "Darek Pobo≈ºniak",
  "license": "ISC",
  "dependencies": {
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.6",
    "redux": "^3.0.5",
    "redux-logger": "^2.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-plugin-react": "^3.15.0",
    "mocha": "^2.3.4",
    "node-sass": "^3.4.2",
    "redux-thunk": "^1.0.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9"
  },
  "keywords": [
    "redux",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DarekPobozniak/react-redux-todo.git"
  },
  "bugs": {
    "url": "https://github.com/DarekPobozniak/react-redux-todo/issues"
  },
  "homepage": "https://github.com/DarekPobozniak/react-redux-todo#readme"
}
